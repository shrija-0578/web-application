         <h2>Table formation</h2>
         <br>
         <table border="1" cellspacing="0" cellpading="0" style="text-align: center;">
            <!-- table tittle -->
            <caption><strong>student marks</strong> </caption>
            <tr>
                <!-- column headders  -->
                <th>name</th>   
                <th>maths</th>
                <th>physics</th>
                <th>chemisry</th>
            </tr>
            <!-- data entered row wise -->
             <tr>
                <td>riya</td>   
                <td>62</td>
                <td>45</td>
                <td>45</td>

             </tr>
