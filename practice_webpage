         <h5>spaning</h5>
         <table border="2">
            <caption>data</caption>
            <tr>
                <th rowspan="2">name</th>
                <th colspan="2">marks</th>
            </tr>
            <tr>
                <th>maths</th>
                <th>english</th>
            </tr>
            <tr>
                <td>riya</td>
                <td>85</td>
                <td>89</td>
            </tr>


         </table>
